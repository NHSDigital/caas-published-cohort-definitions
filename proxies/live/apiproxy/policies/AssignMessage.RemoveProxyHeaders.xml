<AssignMessage async="false" continueOnError="false" enabled="true"
  name="AssignMessage.RemoveProxyHeaders">
  <DisplayName>Remove Specific Proxy Request Headers</DisplayName>
  <Remove>
    <Headers>
      <Header>x-request-id</Header>
      <Header>nhsd-request-id</Header>
      <Header>nhsd-correlation-id</Header>
    </Headers>
  </Remove>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="request" />
</AssignMessage>