<AssignMessage async="false" continueOnError="false" enabled="true"
  name="AssignMessage.RemoveTargetHeaders">
  <DisplayName>Remove Specific Target Request Headers</DisplayName>
  <Remove>
    <Headers>
      <Header>x-request-id</Header>
      <Header>x-correlation-id</Header>
      <Header>nhsd-request-id</Header>
    </Headers>
  </Remove>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="request" />
</AssignMessage>